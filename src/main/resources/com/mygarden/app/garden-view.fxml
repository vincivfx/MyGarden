<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.mygarden.app.controllers.GardenController"
            prefWidth="383.0" prefHeight="433.0">

   <children>

      <!-- Buttons MainPage and Shop, just for testing we can take out if needed  -->
      <Button layoutX="14.0" layoutY="14.0"
              mnemonicParsing="false"
              text="Main Page"
              onAction="#onGoToMainPage"/>

      <Button layoutX="170.0" layoutY="14.0"
              mnemonicParsing="false"
              text="Shop"
              onAction="#onGoToShop"/>

      <!-- If we add an inventory latter -->
      <Button layoutX="303.0" layoutY="14.0"
              mnemonicParsing="false"
              text="Inventory"/>
   </children>
</AnchorPane>
